{
  "name": "shipafreight-test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "unit": "jest --config=jest.config.json --runInBand",
    "watch": "jest --config=jest.config.json --watchAll --runInBand",
    "unit-ci":
      "jest --config=jest.config.json --coverage --runInBand --verbose",
    "acceptance-ci":
      "chimp --browser=phantomjs --ddp=http://localhost:3000 --jasmine --path=tests/acceptance",
    "cover": "jest --config=jest.config.json --coverage",
    "codeclimate-test-reporter": "codeclimate-test-reporter"
  },
  "dependencies": {
    "babel-env": "^2.4.1",
    "babel-jest": "^22.0.4",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "chimp": "^2.0.0-alpha.5",
    "codeclimate-test-reporter": "^0.5.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.0.4",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
